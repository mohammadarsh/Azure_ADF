					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"variableName": "id",
					"value": {
						"value": "@split(pipeline().parameters.input_lst,',')[0]",
						"type": "Expression"
					}
				}
			},
			{
				"name": "act_name_lst",
